<% if @projections.length > 0 %>
$('#available_players').html('<%= j render partial: "table", locals: { collection: @projections } %>');
	<% if @projections.length < 5 %>
		$('.player_name:first a').click();
	<% end %>
	$('#player_search').css({background: "#FFF"});
<% else %>
	$('#player_search').css({background: "#FF0"});
<% end %>